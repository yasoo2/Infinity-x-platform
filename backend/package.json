{
  "name": "joe-ai-development-assistant",
  "version": "2.0.4",
  "description": "JOE X - The Ultimate AI-Powered Development Assistant",
  "main": "server.mjs",
  "type": "module",
  "scripts": {
    "start": "node --require ./src/config-loader.cjs server.mjs",
    "dev": "nodemon --require ./src/config-loader.cjs server.mjs",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ignore-pattern src/systems/code-review.service.mjs",
    "lint:fix": "eslint . --fix --ignore-pattern src/systems/code-review.service.mjs",
    "format": "prettier --write \"**/*.{js,mjs,json,md}\"",
    "check:deadcode": "depcheck",
    "cli": "node src/cli/joe-cli.mjs",
    "init": "node scripts/init.js",
    "demo": "node examples/demo.js"
  },
  "depcheck": {
    "ignores": [
      "@babel/generator",
      "@babel/parser",
      "@babel/traverse",
      "@babel/types",
      "acorn",
      "acorn-walk",
      "aws-sdk",
      "boxen",
      "chalk",
      "chokidar",
      "cli-table3",
      "commander",
      "compression",
      "date-fns",
      "express-mongo-sanitize",
      "express-rate-limit",
      "express-validator",
      "figlet",
      "hpp",
      "inquirer",
      "jimp",
      "joi",
      "lodash",
      "marked",
      "moment",
      "morgan",
      "node-fetch",
      "nodemailer",
      "ora",
      "pdf-lib",
      "pg",
      "pidusage",
      "qrcode",
      "source-map",
      "stack-trace",
      "stripe",
      "twilio",
      "winston",
      "xss-clean",
      "yaml",
      "@babel/preset-env",
      "@babel/preset-typescript",
      "@types/express",
      "@types/node",
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "babel-jest",
      "eslint-config-prettier",
      "eslint-plugin-prettier",
      "mongodb-memory-server",
      "supertest",
      "typescript"
    ]
  },
  "keywords": [
    "ai",
    "development",
    "assistant",
    "code-generator",
    "automation",
    "openai",
    "gpt-4",
    "multi-agent",
    "self-healing"
  ],
  "author": "JOE AI Team",
  "license": "MIT",
