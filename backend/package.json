{
  "name": "joe-ai-manus-advanced",
  "version": "3.0.0",
  "description": "Joe AI - Advanced Development Assistant System (Better Than Manus)",
  "type": "module",
  "main": "backend/server.mjs",
  "scripts": {
    "start": "node backend/server.mjs",
    "dev": "nodemon backend/server.mjs",
    "test": "node backend/test/test-joe-complete.mjs",
    "setup": "node backend/setup.js",
    "setup:complete": "npm run setup && npm run build:all",
    "build:all": "npm run build:dashboard && npm run build:frontend && npm run build:tools",
    "build:dashboard": "cd dashboard-x && npm run build",
    "build:frontend": "cd public-site && npm run build",
    "build:tools": "node backend/build-tools.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:live\" \"npm run dev:sandbox\"",
    "dev:live": "node backend/live-server.js",
    "dev:sandbox": "node backend/sandbox-server.js",
    "test:ai": "node backend/test/test-ai-complete.mjs",
    "test:browser": "node backend/test/test-browser-complete.mjs",
    "test:streaming": "node backend/test/test-streaming.mjs",
    "test:universal": "node backend/test/test-universal-access.mjs",
    "deploy:render": "node backend/deploy/render-deploy.js",
    "deploy:cloudflare": "node backend/deploy/cloudflare-deploy.js",
    "deploy:full": "npm run deploy:render && npm run deploy:cloudflare",
    "monitor": "node backend/monitor/system-monitor.js",
    "logs": "tail -f logs/joe-system.log",
    "backup": "node backend/utils/backup-system.js",
    "restore": "node backend/utils/restore-system.js",
    "health": "node backend/utils/health-check.js",
    "upgrade": "node backend/utils/upgrade-system.js"
  },
  "config": {
    "puppeteer_skip_chromium_download": "false",
    "sandbox_enabled": "true",
    "live_streaming_enabled": "true",
    "universal_access_enabled": "true"
  },
