    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.1.0",
    "helmet": "^7.1.0",
    "json-merger": "^1.1.10",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "mongoose": "^8.3.2",
    "openai": "^4.104.0",
    "puppeteer": "^22.9.0",
