{
  "name": "joengine",
  "version": "2.4.0",
  "main": "backend/server.mjs",
  "type": "module",
  "scripts": {
    "start": "node backend/server.mjs",
    "dev": "nodemon backend/server.mjs --watch backend --ext mjs,json",
    "lint": "eslint --ignore-pattern backend/src/systems/code-review.service.mjs backend/src dashboard-x/src",
    "postinstall": "npx puppeteer browsers install chrome || true",
    "deploy:prod:backend": "if [ -z \"$RENDER_API_KEY\" ] || [ -z \"$RENDER_SERVICE_ID\" ]; then echo 'Missing RENDER_API_KEY or RENDER_SERVICE_ID'; exit 1; else curl -s -X POST -H \"Authorization: Bearer $RENDER_API_KEY\" https://api.render.com/v1/services/$RENDER_SERVICE_ID/deploys | jq .; fi",
    "prepare": "husky",
    "auto-push": "node scripts/auto-push.mjs"
  },
